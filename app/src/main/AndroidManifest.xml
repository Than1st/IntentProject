<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.than.intentproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IntentProject">
        <activity
            android:name=".IntentParcelableDataActivity"
            android:exported="false" />
        <activity
            android:name=".IntentSerializableDataActivity"
            android:exported="false" />
        <activity
            android:name=".IntentBundleDataActivity"
            android:exported="false" />
        <activity
            android:name=".IntentWithDataActivity"
            android:exported="false" />
        <activity
            android:name=".IntentNoDataActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>